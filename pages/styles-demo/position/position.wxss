/* pages/styles-demo/position/position.wxss */
@import "../../common/index.wxss";
text {
    margin: 18px;
}

.example {
    position: relative;
    width: 100%;
    padding: 100rpx 0;
    margin: 100rpx 0;
    background: #eee;
}

.border {
    border: solid 1px #666;
}

.size-big {
    width: 200rpx;
    height: 200rpx;
    text-align: center;
    /* margin: auto; */
}

.size-normal {
    width: 100rpx;
    height: 100rpx;
    text-align: center;
    /* margin: auto; */
}

.green-1 {
    background-color: rgb(0, 155, 0);
}

.green-2 {
    background-color: rgb(50, 185, 50);
}

.green-3 {
    background-color: rgb(100, 215, 100);
}

.green-4 {
    background-color: rgb(150, 255, 150);
}